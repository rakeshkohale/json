		$(document).ready(function(){
			var varContinent = JSONData.continents;
			for(var i in varContinent){
				var varCountry =varContinent[i].countries;
			for(var j in varCountry){
				var varRegion =varCountry[j].regions;
			for(var r in varRegion){
				$("#countryDetails").append("<tr> <td>"+i
					+"</td> <td>"+j
					+"</td> <td>"+varRegion[r]
					+"</td> </tr>");
					}
					}
				}
			});
